<%@ page language="java" pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
<!DOCTYPE html>
<html>
<head>
<jsp:include page="/resources/inc/meta.jsp"></jsp:include>
<jsp:include page="/resources/inc/easyui.jsp"></jsp:include>
</head>
<body>
<script type="text/javascript">
function preview(id){  
    var simg = $('#'+id).val();;  
    wins = $("<div align='center' style='text-align:center; background:#90A4AE'><img id='simg' width='100%' height='auto'/></div>").dialog({
		title:'大图预览',
		width:'95%',
		height:'95%',
		maximizable:true,
		modal:true,
		onClose:function(){
	    	$(this).dialog("destroy");
	    },
	});
    $("#simg").attr("src",simg);  
}
</script>
<div align="left" style="margin-top: 10px; font-size: 13px;">
 	<form id="fangzhiConfigForm" method="post">
 		<input name="id" type="hidden" value="${fangzhiConfigVo.id}"/>
		<table class="tableForm" style="width:60%;">
			<tr>
				<th>链接 URL：</th>
				<td><input  name="link" value="${fangzhiConfigVo.link}" class="easyui-textbox" data-options="required:true,readonly:false" style="width: 100%;" /></td>
			</tr>
			<tr>
				<th>上传图片：</th>
				<td >
					<div style="height:120px">
						<c:if test="${not empty fangzhiConfigVo.image}">
							<img id="image_img_1" onclick="uploadImg('image_img_1','image','bjqlds')" src="${fangzhiConfigVo.image}" style="width:120px;height:120px" />
						</c:if>
						<c:if test="${empty fangzhiConfigVo.image}">
							<img id="image_img_1" onclick="uploadImg('image_img_1','image','bjqlds')" src="data:image/png;base64," style="width:160px;height:120px"/>
						</c:if>
						<input id="image" name="image" value="${fangzhiConfigVo.image}" type="hidden"/>
					</div>
					<c:if test="${not empty fangzhiConfigVo.image}"><a href="javascript:preview('image')">查看大图</a></c:if>
				</td>
			</tr>
			<tr>
				<td></td>
				<td align="right"><a href="javascript:void(0)" onclick="edit(0)">更新</a></td>
			</tr>
		</table>
		
	</form>
</div>
</body>
<script type="text/javascript" charset="UTF-8" src="<%=request.getContextPath()%>/resources/js/applyhm/fangzhi/config.js?date=<%=System.currentTimeMillis() %>"></script>
</html>

